@model IList<VilaStella.WebAdminClient.Models.ReservationsOutputModel>

@{
    ViewBag.Title = "Вила Стела";
}

<div class="main-container">
    @if (Model.Count == 0)
    {
        <h3 class="text-center">В момента няма нови резервации</h3>
    }
    else
    {
        @Html.Partial("_ReservationsTable", Model)
    }
</div>

@section scripts
{
    <script src="~/Scripts/App/Admin/helpers.js"></script>
}